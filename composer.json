{
  "name": "techworker/fn-sortby",
  "type": "library",
  "description": "Small function that can be used to sort an array of any shape. PHP7 only.",
  "keywords": ["functional", "programming", "sorting"],
  "homepage": "http://www.techworker.de/fn",
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Ansbach",
      "email": "ben@techworker.de"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.4"
  },
  "autoload": {
    "files": [
      "src/SortBy.php"
    ],
    "psr-4": {
      "techworker\\fn\\sortBy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "techworker\\fn\\sortBy\\tests\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}