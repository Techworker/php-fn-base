{
  "name": "techworker/php-fn-sortby",
  "type": "library",
  "description": "Samll function that can be used to sort any kind of data structure. PHP7 only.",
  "keywords": ["functional", "programming", "sort"],
  "homepage": "http://www.techworker.de/fn",
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Ansbach",
      "email": "ben@techworker.de"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.4"
  },
  "autoload": {
    "files": [
      "src/sortBy.php"
    ],
    "psr-4": {
      "techworker\\fn\\sortby\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "techworker\\fn\\sortby\\tests\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
